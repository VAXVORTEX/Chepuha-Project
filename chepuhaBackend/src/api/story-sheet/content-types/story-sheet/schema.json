{
  "kind": "collectionType",
  "collectionName": "story_sheets",
  "info": {
    "singularName": "story-sheet",
    "pluralName": "story-sheets",
    "displayName": "StorySheet"
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "sheet_id": {
      "type": "uid",
      "private": true,
      "required": true
    },
    "session_id": {
      "type": "uid"
    },
    "original_owner_id": {
      "type": "uid"
    },
    "current_owner_id": {
      "type": "uid"
    },
    "sheet_number": {
      "type": "integer"
    },
    "storysheets_status": {
      "type": "enumeration",
      "enum": [
        "in_progress",
        "completed"
      ]
    },
    "final_story": {
      "type": "string"
    },
    "storysheets_created_at": {
      "type": "datetime"
    },
    "storysheets_completed_at": {
      "type": "datetime"
    }
  }
}
