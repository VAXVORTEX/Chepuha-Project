{
  "kind": "collectionType",
  "collectionName": "story_sheets",
  "info": {
    "singularName": "story-sheet",
    "pluralName": "story-sheets",
    "displayName": "StorySheet"
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "sheet_id": {
      "type": "uid",
      "private": true,
      "required": true
    },
    "sheet_number": {
      "type": "integer"
    },
    "storysheets_status": {
      "type": "enumeration",
      "enum": [
        "in_progress",
        "completed"
      ]
    },
    "final_story": {
      "type": "string"
    },
    "storysheets_created_at": {
      "type": "datetime"
    },
    "storysheets_completed_at": {
      "type": "datetime"
    },
    "game_session": {
      "type": "relation",
      "relation": "manyToOne",
      "target": "api::game-session.game-session",
      "inversedBy": "story_sheets"
    },
    "player": {
      "type": "relation",
      "relation": "manyToOne",
      "target": "api::player.player",
      "inversedBy": "story_sheets"
    },
    "answers": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::answer.answer",
      "mappedBy": "story_sheet"
    }
  }
}
