{
  "kind": "collectionType",
  "collectionName": "rounds",
  "info": {
    "singularName": "round",
    "pluralName": "rounds",
    "displayName": "Rounds"
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
