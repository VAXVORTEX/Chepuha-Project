        "who",
        "when",
        "where",
        "with_whom",
        "what_did",
        "what_said",
        "how_ended"
      ]
    },
    "rounds_status": {
      "type": "enumeration",
      "enum": [
        "pending",
        "active",
        "completed"
      ]
    },
    "started_at": {
      "type": "datetime"
    },
    "completed_at": {
      "type": "datetime"
    }
  }
