{
  "kind": "collectionType",
  "collectionName": "players",
  "info": {
    "singularName": "player",
    "pluralName": "players",
    "displayName": "players"
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "player_id": {
      "type": "uid",
      "private": true,
      "required": true
    },
    "nickname": {
      "type": "string"
    },
    "player_order": {
      "type": "integer"
    },
    "players_status": {
      "type": "enumeration",
      "enum": [
        "joined",
        "ready",
        "playing",
        "finished"
      ]
    },
    "joined_at": {
      "type": "datetime"
    },
    "session_id": {
      "type": "relation",
      "relation": "manyToOne",
      "target": "api::game-session.game-session",
      "inversedBy": "players"
    },
    "story_sheets": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::story-sheet.story-sheet",
      "mappedBy": "player"
    },
    "answers": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::answer.answer",
      "mappedBy": "player"
    }
  }
}
