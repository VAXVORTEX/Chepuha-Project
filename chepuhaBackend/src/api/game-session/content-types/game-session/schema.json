{
  "kind": "collectionType",
  "collectionName": "game_sessions",
  "info": {
    "singularName": "game-session",
    "pluralName": "game-sessions",
    "displayName": "gameSession"
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "session_id": {
      "type": "uid",
      "private": true,
      "required": true
    },
    "session_name": {
      "type": "string"
    },
    "session_status": {
      "type": "enumeration",
      "enum": [
        "waiting",
        "active",
        "completed"
      ]
    },
    "max_players": {
      "type": "integer"
    },
    "current_players_count": {
      "type": "integer"
    },
    "session_created_at": {
      "type": "datetime"
    },
    "session_started_at": {
      "type": "datetime"
    },
    "session_ended_at": {
      "type": "datetime"
    },
    "rounds": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::round.round",
      "mappedBy": "session_id"
    }
  }
}
